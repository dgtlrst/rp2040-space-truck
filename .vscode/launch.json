{
    "version": "0.2.0",
    "configurations": [
        {
            "preLaunchTask": "${defaultBuildTask}",
            "type": "probe-rs-debug",
            "request": "launch",
            "name": "probe-rs-debugger-test",
            "chip": "RP2040",
            "connectUnderReset": false,
            "coreConfigs": [
                {
                    "coreIndex": 0,
                    "programBinary": "${workspaceFolder}/target/thumbv6m-none-eabi/debug/embassy_rp_w_template",
                    "svdFile": "${workspaceFolder}/svd_files/RP2040.svd",
                    "rttEnabled": true,
                    "rttChannelFormats": [
                        {
                            "dataFormat": "String",
                            "channelNumber": 0,
                            "showTimestamps": true
                        },
                        {
                            "dataFormat": "BinaryLE",
                            "channelNumber": 1
                        }
                    ]
                }
            ],
            "flashingConfig": {
                "flashingEnabled": true,
                "haltAfterReset": true
            },
            "consoleLogLevel": "Debug",
            "wireProtocol": "Swd",
            "runtimeExecutable": "probe-rs",
            "runtimeArgs": [
                "dap-server"
            ]
        }
    ]
}